CREATE LOCAL TEMP TABLE PROM_ORDER_INFO_A  ON COMMIT PRESERVE ROWS
AS ( SELECT
	    *
        FROM
        (
        SELECT   COALESCE(P2.ORDER_PROM_ROW_ID,'-1') AS ORDER_PROM_ROW_ID
                  ,P1.PROM_ROW_ID
                            ,P1.PROM_TYPE_NAME
                            ,P1.PROM_EFF_DT
                            ,P1.PROM_EXP_DT
                            ,P1.PROM_INTEG_ID
                            ,P1.PROM_ASSET_INTEG_ID
                            ,P1.STAT_NAME
                            ,P1.LATN_ID
                            ,P1.PROM_INSTANT_ROW_ID
